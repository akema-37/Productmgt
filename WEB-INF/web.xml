<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE web-app
PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
	<servlet>
		<servlet-name>UserAuthServlet</servlet-name>
		<servlet-class>com.rosy.sample.UserAuthServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UserAuthServlet</servlet-name>
		<url-pattern>/UserAuthServlet</url-pattern>
	</servlet-mapping>
	<session-config>
		<session-timeout>30</session-timeout>
	</session-config>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>SearchServlet</servlet-name>
		<servlet-class>com.rosy.sample.SearchServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SearchServlet</servlet-name>
		<url-pattern>/SearchServlet</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>DetailServlet</servlet-name>
		<servlet-class>com.rosy.sample.DetailServlet</servlet-class>

	</servlet>
	<servlet-mapping>
		<servlet-name>DetailServlet</servlet-name>
		<url-pattern>/DetailServlet</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>UpdateServlet</servlet-name>
		<servlet-class>com.rosy.sample.UpdateServlet</servlet-class>

	</servlet>
	<servlet-mapping>
		<servlet-name>UpdateServlet</servlet-name>
		<url-pattern>/UpdateServlet</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>InsertServlet</servlet-name>
		<servlet-class>com.rosy.sample.InsertServlet</servlet-class>

	</servlet>
	<servlet-mapping>
		<servlet-name>InsertServlet</servlet-name>
		<url-pattern>/InsertServlet</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>UserInsertServlet</servlet-name>
		<servlet-class>com.rosy.sample.UserInsertServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>UserInsertServlet</servlet-name>
		<url-pattern>/UserInsertServlet</url-pattern>
	</servlet-mapping>


</web-app>